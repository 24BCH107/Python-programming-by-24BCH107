def factorial(n):
result = 1
for y in range(1, n + 1):
result *= y
return result
def nCr(n, r):
return factorial(n) 
def nPr(n, r):
return factorial(n) 
2
n = int(input("Enter the value of n: "))
r = int(input("Enter the value of r: "))
print(f"{n}C{r} = {nCr(n, r)}")
print(f"{n}P{r} = {nPr(n, r)}"
